# iceOS â€“ staging environment example

# Auth
ICE_ALLOW_DEV_TOKEN=0
ICE_API_TOKEN=

# CORS/Hosts
CORS_ORIGINS=
ALLOW_CORS_WILDCARD=0
TRUSTED_HOSTS=

# DB/Redis
DATABASE_URL=postgresql+asyncpg://iceos:iceos@postgres:5432/iceos
ICEOS_RUN_MIGRATIONS=1
ICEOS_REQUIRE_DB=1
REDIS_URL=redis://redis:6379/0

# Observability (disabled by default)
ICEOS_ENABLE_METRICS=0
# OTEL_EXPORTER_OTLP_ENDPOINT=

# RAG
ICEOS_EMBEDDINGS_PROVIDER=hash

# Rate limiting
ICE_RATE_WINDOW_SECONDS=10
ICE_RATE_MAX_REQUESTS=60
